{
    "installCommand":"npm install --legacy-peee-deps",
    "version":2,
    "builds": [
        {
            "src": "server.js",
            "use":"@vercel/node"
        },
        {
            "src": "src/**/*",
            "use":"@vercel/static"
        }
    ],
    "routes":[
        {"src":"/(,*)","dest":"/"}
    ]
}